{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "standard"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaversion": 6,
    "sourceType": "module"
  },
  "rules": {
    "indent": [2,2],
    "linebreak-style": [2,"unix"],
    "quotes": [2,"single"],
    "semi": [2,"never"]
  }
}
